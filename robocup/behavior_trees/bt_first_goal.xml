<?xml version="1.0"?>
<root main_tree_to_execute="BehaviorTree">
    <!-- ////////// -->
    <BehaviorTree ID="BehaviorTree">
        <SequenceStar>
            <Action ID="SendPoint" point="{point}"/>
            <Action ID="Move" point="{point}"/>
        </SequenceStar>
    </BehaviorTree>
    <!-- ////////// -->
    <TreeNodesModel>
        <Action ID="Move">
            <input_port name="{point}"/>
        </Action>
        <Action ID="SendPoint">
            <output_port name="{point}"/>
        </Action>
    </TreeNodesModel>
    <!-- ////////// -->
</root>

